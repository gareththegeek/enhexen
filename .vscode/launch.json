  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm start",
      "name": "Importer",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}/importer"
    },
    {
      "type": "firefox",
      "request": "launch",
      "name": "Front-end",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/enhexen/src",
      "pathMappings": [
        {
          "url": "http://localhost:5173/src",
          "path": "${workspaceFolder}/enhexen/src"
        }
      ]
    }
  ]
}